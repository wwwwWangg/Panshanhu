<Root>
  <NeedLogin>False</NeedLogin>
  <MaxThreadNum>1</MaxThreadNum>
  <StartUrls>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=1</Url>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=2</Url>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=3</Url>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=4</Url>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=5</Url>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=6</Url>
    <Url>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=7</Url>
  </StartUrls>
  <BrowserSetting>
    <DisablePrompt>True</DisablePrompt>
    <DisableImg>False</DisableImg>
    <DisableJS>False</DisableJS>
    <DisableFlash>False</DisableFlash>
    <AdBlock>True</AdBlock>
    <ProxyPort>8088</ProxyPort>
  </BrowserSetting>
  <Pages>
    <Page>
      <RowXPath>//*[@id="con"]/div[1]/div</RowXPath>
      <NoScript>False</NoScript>
      <EngineMode>1</EngineMode>
      <RowMode>1</RowMode>
      <TestUrl>http://news.baidu.com/n?cmd=4&amp;class=civilnews&amp;pn=1</TestUrl>
      <PaginationMode>0</PaginationMode>
      <PaginationMaxNum>0</PaginationMaxNum>
      <Fields>
        <Field>
          <Name>title</Name>
          <Attribute>Text</Attribute>
          <DeepIn>True</DeepIn>
          <UniqueIndex>False</UniqueIndex>
          <Required>False</Required>
          <XPath>./a[1]</XPath>
        </Field>
        <Field>
          <Name>title_Link</Name>
          <Attribute>href</Attribute>
          <DeepIn>True</DeepIn>
          <UniqueIndex>False</UniqueIndex>
          <Required>False</Required>
          <XPath>./a[1]</XPath>
        </Field>
        <Field>
          <Name>time</Name>
          <Attribute>Text</Attribute>
          <DeepIn>True</DeepIn>
          <UniqueIndex>False</UniqueIndex>
          <Required>False</Required>
          <XPath>./span[1]</XPath>
        </Field>
      </Fields>
    </Page>
  </Pages>
</Root>